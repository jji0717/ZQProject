     <!-- Streamer::PlayList -->
     <Handler enabled="0" name="OnItemStepped" target="jboss1.PlayList" source="TianShan/Event/Streamer/Playlist">
        <filter eventName="OnItemStepped" category="PlayList" sourceNetId=".*">
