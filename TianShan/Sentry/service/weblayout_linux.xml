    <property name="SITEADMIN_EP" value="${SiteAdminEndpoint}"/>
    
    <property name="NSS_StorageEP" value="tcp -h ${ServerNetIf} -p 10800"/>
    <property name="GBVSS_StorageEP" value="tcp -h ${ServerNetIf} -p 10800"/>
    <property name="C2SS_StorageEP" value="tcp -h ${ServerNetIf} -p 10801"/>
