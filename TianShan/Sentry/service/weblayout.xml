      <param name="log#filename" value="${LOG_DIR}\SentryHttp.log"/>
      <param name="log#filetype" value="filelog"/>
      <param name="log#pagesize" value="${LOG_PAGESIZE}"/>
      <param name="log#pageidx" value="-1"/>
    </page>
